		"@sanity/client": "^2.2.6",
		"@types/three": "^0.125.1",
		"@types/vimeo__player": "^2.10.0",
		"@types/youtube-player": "^5.5.3",
		"@vimeo/player": "^2.15.0",
		"autoprefixer": "^10.3.7",
		"cssnano": "^5.0.2",
		"groq": "^2.2.6",
		"html-minifier": "^4.0.0",
		"loader-utils": "^2.0.0",
		"markdown-it": "^10.0.0",
		"markdown-it-emoji": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.3.9",
		"postcss-cli": "^8.3.1",
		"postcss-hash": "^2.0.0",
		"postcss-import": "^14.0.1",
		"postcss-import-ext-glob": "^2.0.1",
		"postcss-loader": "^5.2.0",
		"postcss-preset-env": "^6.7.0",
